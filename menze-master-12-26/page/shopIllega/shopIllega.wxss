page {
  background-color: #fbf9fe;
}
.section__title {
    font-family: "Microsoft YaHei UI";
    font-size: 36rpx;
    font-weight: bold;
    padding: 15rpx;
    margin: 15rpx;
}
.label-1{
    padding: 20rpx;
}
.label-1__text{
    width: 70%;
    color: gray;
    display: inline-block;
    vertical-align: middle;
}

.label-1__icon {
    position: relative;
    margin-right: 10px;
    display: inline-block;
    vertical-align: middle;
    border-radius:9px;
    width: 20px;
    height: 20px;
    background: white;
}
.button-wrapper {
  padding: 30rpx;
  margin-top: 10rpx;
}
.picker{
    padding: 26rpx;
    background-color: white;
    color: gray;
}
.label-1__icon-checked {
    position: absolute;
    top: -3px;
    /*left: 3px;*/
    width: 12px;
    height: 12px;
    /*background: #1aad19;*/
}

.input {
    margin-top: 10px;
    padding: 20rpx;
    background-color: white;
}

.images-wrapper {
  padding: 20rpx;
  background-color: #fff;
  margin-top: 20rpx;
}
.images-wrapper-text {
  font-size: 28rpx;
}
.images-list {
  display: flex;
  margin-top: 20rpx;
  flex-wrap: wrap;
}
.images-image {
  width: 150rpx;
  height: 150rpx;
  margin: 10rpx;
}
.image-plus {
  border: 1px solid #999;
  position: relative;
}
.image-plus-horizontal {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 4rpx;
  height: 80rpx;
  transform: translate(-50%, -50%);
}
.image-plus-vertical {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 80rpx;
  height: 4rpx;
  transform: translate(-50%, -50%);
}
.loadingGIFView {
    position: absolute;
    top: 50%;
    left: 23.35%;
}
.loadingImage {
    width: 200px;
    height: 150px;
}
.bgView {
    position: absolute;
    top: 0rpx;
    width: 100%;
    height: 120%;
    background-color: lightgrey;
    opacity: 0.5;
}
.commitBtn{
    background-color: #27a59c;
    color: white;
}